{
  "name": "parcels-for-kaios",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "dependencies": {},
  "scripts": {
    "start": "parcel public/index.html --port 8080",
    "build": "parcel build public/index.html",
    "lint": "tslint -p ./tsconfig.json",
    "check": "tsc -p ./tsconfig.json --noEmit",
    "test": "yarn run check && yarn run lint"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@parcel/packager-ts": "2.9.3",
    "@parcel/transformer-babel": "^2.8.3",
    "@parcel/transformer-sass": "^2.8.3",
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@parcel/transformer-typescript-types": "2.9.3",
    "babel-plugin-inferno": "^6.6.0",
    "classnames": "^2.3.2",
    "inferno": "^8.2.2",
    "inferno-extras": "^8.0.6",
    "parcel": "^2.8.3",
    "ts-plugin-inferno": "^1.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "classnames": "^2.3.2",
    "inferno": "^8.2.2",
    "inferno-extras": "^8.2.2"
  },
  "browserslist": "Firefox 48",
  "targets": {
    "main": {
      "isLibrary": false,
      "source": "src/index.tsx",
      "outputFormat": "commonjs",
      "includeNodeModules": {
        "inferno": false,
        "inferno-extras": false,
        "classnames": false
      }
    }
  }
}